   "source": [
    "import cv2\n",
    "\n",
    "\n",
    "image = cv2.imread(\"ALIU_ABDULMUJEEB_OLOLADE.jpg\")\n",
    "\n",
    "larger = cv2.pyrUp(image)\n",
    "smaller = cv2.pyrDown(larger)\n",
    "\n",
    "\n",
    "cv2.imshow(\"Smaller Image\",smaller)\n",
    "cv2.waitKey(0)\n",
    "\n",
    "cv2.imshow(\"Larger Image\",larger)\n",
    "cv2.waitKey(0)\n",
    "\n",
    "cv2.destroyAllWindows()\n"
   ]
